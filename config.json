{
    "Logger": {
        "Level": "info",
        "Formatter": "json"
    },
    "HTTP": {
        "Addr": ":8080",
        "Endpoints": [
            {
                "Path": "/api/v1/tickers/stream",
                "Method": "get",
                "Queue": {
                    "Type": "nsq",
                    "Nsq": {
                        "Addr": "127.0.0.1:4150",
                        "Topic": "ticker",
                        "Channel": "platypus",
                        "ConsumerBufferSize": 0,
                        "LogLevel": 1
                    }
                },
                "Consumer": {"Format": "json"},
                "Store": {"Type": "memoryttl"},
                "Transmitter": {"Type": "broadcast"}
            }
        ]
    }
}
